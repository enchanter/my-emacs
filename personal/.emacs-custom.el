 '(font-lock-function-name-face ((((class color) (background dark)) (:foreground "green2"))))
 '(font-lock-keyword-face ((((class color) (background dark)) (:foreground "dark cyan"))))
 '(font-lock-string-face ((((class color) (background dark)) (:foreground "darkorange"))))
 '(font-lock-variable-name-face ((((class color) (background dark)) (:foreground "goldenrod1"))))
